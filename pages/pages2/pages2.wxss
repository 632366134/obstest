@keyframes change {
    0% {
        filter: drop-shadow(0 0  0 rgba(255, 255, 255, 0.7));

    }

    100% {
        filter: drop-shadow(0 0 50px  rgba(255, 255, 255,0.7))
    }
}

@keyframes change2 {
    0% {
        scale:0.5;
        transform: rotateY(0deg);
    }

    /* 25% {
        scale: 1;
        transform: rotateY(90deg);
    } */

    50% {
        scale: 1.2;

        transform: rotateY(180deg);
    }

    /* 85% {
        scale: 0.8;
        transform: rotateY(270deg);

    } */

    100% {
        scale: 1;
        transform: rotateY(360deg);
    }
}

.imageLight {
    animation: change 2.5s ease;
}

.imageLight2 {
    animation: change2 1s ease-out;


@keyframes move_eye {
    from {
        margin-bottom: 20rpx;
    }

    to {
        margin-bottom: 50rpx;
    }
}

.container2 {
    position: fixed;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: flex-end;
    box-sizing: border-box;
    background-size: 100% 100%;
    background-image: url("data:image/png;base64,");
}

.container2 .back {
    flex-direction: column;
    text-align: center;
    position: fixed;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 99999;
    width: auto;height: auto;
}
.container2 .bigBack {
    width: 110rpx;
    height: 110rpx;
    background: rgba(161, 172, 253, 0.5);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
}
.container2 .smallBack {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 80rpx;
    height: 80rpx;
    background: rgba(161, 172, 253, 0.5);

    opacity: 0.5;
    border-radius: 50%;

}
.container2 .bigBack2 {
  
    background: rgba(161, 172, 253,0.8);

}
.container2 .smallBack2 {
    background: rgba(161, 172, 253, 1);

}
